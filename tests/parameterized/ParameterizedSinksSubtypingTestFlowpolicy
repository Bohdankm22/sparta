
READ_SMS -> FILESYSTEM("/usr/bin")
READ_SMS -> FILESYSTEM("/usr/*")
READ_SMS -> FILESYSTEM("/dev/null/", "/usr/*")
READ_SMS -> FILESYSTEM("myfile2.txt", "myfile.txt")
READ_SMS -> FILESYSTEM("myfile.txt")
READ_SMS -> INTERNET("*.google.com")
READ_SMS -> INTERNET("maps.google.com")
READ_SMS -> INTERNET("voice.google.com")
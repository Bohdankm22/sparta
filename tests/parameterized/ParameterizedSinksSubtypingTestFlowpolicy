LITERAL -> CONDITIONAL,INTERNET
ANY -> CONDITIONAL("/usr/bin")
ANY -> CONDITIONAL("/usr/*")
ANY -> CONDITIONAL("/dev/null/", "/usr/*")
ANY -> CONDITIONAL("myfile2.txt", "myfile.txt")
ANY -> CONDITIONAL("myfile.txt")
ANY -> INTERNET("*.google.com")
ANY -> INTERNET("maps.google.com")
ANY -> INTERNET("voice.google.com")